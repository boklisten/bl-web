{
	"compileOnSave": false,
	"compilerOptions": {
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"target": "ESNext",
		"typeRoots": ["node_modules/@types"],
		"lib": ["ESNext", "dom"],
		"types": ["node"]
	}
}
