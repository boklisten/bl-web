{
	"name": "bl-web",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "node server.js",

		"serve:local": "npm run build:local && node server.js",
		"serve:dev": "npm run build:dev && node server.js",

		"extract-i18n": "ng xi18n --output-path src/locale --locale en && xliffmerge --profile xliffmerge.json en nb",

		"watch": "ng serve --aot --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en --env=&{ANGULAR_ENV}",
		"watch-nb": "ng serve --aot --i18nFile=src/locale/messages.nb.xlf --i18nFormat=xlf --locale=nb --env=${ANGULAR_ENV}",

		"build": "ng build --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en --prod --env=${ANGULAR_ENV}",
		"build-nb": "ng build --i18nFile=src/locale/messages.nb.xlf --i18nFormat=xlf --locale=nb --prod --env=${ANGULAR_ENV}",

		"postinstall": "npm run build-nb",

		"test": "ng test",

		"lint": "ng lint",

		"e2e": "ng e2e"
	},
	"engines": {
		"node": "8.4.0",
		"npm": "5.7.1"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^5.2.5",
		"@angular/cli": "1.7.4",
		"@angular/common": "^5.2.5",
		"@angular/compiler": "^5.2.5",
		"@angular/compiler-cli": "^5.2.0",
		"@angular/core": "^5.2.5",
		"@angular/forms": "^5.2.5",
		"@angular/http": "^5.2.5",
		"@angular/platform-browser": "^5.2.5",
		"@angular/platform-browser-dynamic": "^5.2.5",
		"@angular/router": "^5.2.5",
		"@fortawesome/angular-fontawesome": "0.1.0-10",
		"@fortawesome/fontawesome-svg-core": "^1.2.0-11",
		"@fortawesome/free-brands-svg-icons": "^5.1.0-11",
		"@fortawesome/free-regular-svg-icons": "^5.1.0-11",
		"@fortawesome/free-solid-svg-icons": "^5.1.0-11",
		"@ng-bootstrap/ng-bootstrap": "^1.0.0",
		"@wizardcoder/bl-connect": "^0.5.10",
		"@wizardcoder/bl-login": "^0.1.2",
		"@wizardcoder/bl-model": "^0.14.7",
		"bootstrap": "4.0.0",
		"core-js": "^2.5.3",
		"email-validator": "^1.1.1",
		"express": "^4.16.3",
		"moment": "^2.22.2",
		"moment-es6": "^1.0.0",
		"npm": "^6.0.1",
		"rxjs": "^5.5.6",
		"zone.js": "^0.8.20"
	},
	"devDependencies": {
		"@angular/language-service": "^5.2.0",
		"@types/jasmine": "~2.8.3",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "~6.0.60",
		"codelyzer": "^4.0.1",
		"jasmine-core": "~2.8.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~2.0.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ngx-i18nsupport": "^0.16.2",
		"protractor": "~5.1.2",
		"ts-node": "~4.1.0",
		"tslint": "~5.9.1",
		"typescript": "~2.5.3"
	}
}
