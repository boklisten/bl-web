{
	"name": "bl-web",
	"version": "1.9.2",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "node server.js",
		"extract-i18n": "ng xi18n && xliffmerge --profile xliffmerge.json en nb",
		"serve:local": "npm run build:local && node server.js",
		"serve:dev": "npm run build:dev && node server.js",
		"serve:prod": "npm run build:prod && node server.js",
		"watch": "gulp test && ng serve  --host 0.0.0.0 --hmr",
		"watch-nb": "gulp test && ng serve  --host 0.0.0.0 --hmr --configuration=nb",
		"build": "gulp ${NODE_ENV} && ng build --configuration=${ANGULAR_ENV}",
		"build:local": "gulp test && ng build --configuration=local-nb",
		"build:dev": "gulp development && ng build --configuration=dev-nb",
		"build:prod": "gulp production && ng build --configuration=production-nb",
		"postinstall": "npm run build",
		"test": "ng test --sourceMap=false",
		"test:coverage": "ng test --code-coverage",
		"test-ci": "ng test --karma-config=karma.ci.conf.js",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"engines": {
		"node": "8.11.3",
		"npm": "6.1.0"
	},
	"resolutions": {
		"uglify-es": "npm:terser"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "6.0.6",
		"@angular/cli": "^7.2.0",
		"@angular/common": "6.0.6",
		"@angular/compiler": "6.0.6",
		"@angular/compiler-cli": "6.0.6",
		"@angular/core": "6.0.6",
		"@angular/forms": "6.0.6",
		"@angular/http": "6.0.6",
		"@angular/platform-browser": "6.0.6",
		"@angular/platform-browser-dynamic": "6.0.6",
		"@angular/router": "6.0.6",
		"@fortawesome/angular-fontawesome": "^0.1.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.2",
		"@fortawesome/free-brands-svg-icons": "^5.2.0",
		"@fortawesome/free-regular-svg-icons": "^5.2.0",
		"@fortawesome/free-solid-svg-icons": "^5.2.0",
		"@ng-bootstrap/ng-bootstrap": "^2.1.2",
		"@wizardcoder/bl-connect": "^0.16.1",
		"@wizardcoder/bl-login": "^0.7.14",
		"@wizardcoder/bl-model": "^0.23.2",
		"bootstrap": "^4.4.1",
		"core-js": "^2.5.3",
		"email-validator": "^1.1.1",
		"express": "^4.16.3",
		"file-saver": "^2.0.2",
		"moment": "^2.22.2",
		"moment-es6": "^1.0.0",
		"ng-click-outside": "^5.1.1",
		"npm": "^6.13.1",
		"rxjs": "^6.2.1",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.13.4",
		"@angular/language-service": "6.0.6",
		"@angularclass/hmr": "^2.1.3",
		"@types/jasmine": "^3.3.4",
		"@types/node": "~6.0.60",
		"codelyzer": "^4.0.1",
		"gulp": "^4.0.2",
		"gulp-download-stream": "0.0.19",
		"jasmine-core": "^3.3.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^3.1.4",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-firefox-launcher": "^1.1.0",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-phantomjs-launcher": "^1.0.4",
		"ngx-i18nsupport": "^0.16.2",
		"prettier": "^1.15.3",
		"protractor": "^5.4.0",
		"puppeteer": "^1.20.0",
		"terser": "^3.10.11",
		"ts-node": "~4.1.0",
		"tslint": "~5.9.1",
		"typescript": "^2.7.2"
	}
}
