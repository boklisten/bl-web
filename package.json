{
	"name": "@boklisten/bl-web",
	"version": "1.10.15",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "node server.js",
		"extract-i18n": "ng xi18n && xliffmerge --profile xliffmerge.json en nb",
		"serve:local": "npm run build:local && node server.js",
		"serve:dev": "npm run build:dev && node server.js",
		"serve:prod": "npm run build:prod && node server.js",
		"watch": "gulp test && ng serve  --host 0.0.0.0 --hmr",
		"watch-nb": "gulp test && ng serve  --host 0.0.0.0 --hmr --configuration=nb",
		"build": "gulp ${NODE_ENV} && ng build --configuration=${ANGULAR_ENV}",
		"build:local": "gulp test && ng build --configuration=local-nb",
		"build:dev": "gulp development && ng build --configuration=dev-nb",
		"build:prod": "gulp production && ng build --configuration=production-nb",
		"postinstall": "npm run build",
		"test": "ng test --sourceMap=false",
		"test:coverage": "ng test --code-coverage",
		"test-ci": "ng test --karma-config=karma.ci.conf.js",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"prettier": "prettier --write . --ignore-path=.gitignore"
	},
	"resolutions": {
		"uglify-es": "npm:terser"
	},
	"dependencies": {
		"@angular/animations": "11.2.3",
		"@angular/cli": "^11.2.2",
		"@angular/common": "^11.2.3",
		"@angular/compiler": "^11.2.3",
		"@angular/compiler-cli": "^11.2.3",
		"@angular/core": "^11.2.3",
		"@angular/forms": "^11.2.3",
		"@angular/localize": "^11.2.3",
		"@angular/platform-browser": "^11.2.3",
		"@angular/platform-browser-dynamic": "^11.2.3",
		"@angular/router": "^11.2.3",
		"@boklisten/bl-connect": "^0.19.0",
		"@boklisten/bl-login": "^1.1.5",
		"@boklisten/bl-model": "^0.25.0",
		"@fortawesome/angular-fontawesome": "^0.8.2",
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@fortawesome/free-brands-svg-icons": "^5.15.2",
		"@fortawesome/free-regular-svg-icons": "^5.15.2",
		"@fortawesome/free-solid-svg-icons": "^5.15.2",
		"@ng-bootstrap/ng-bootstrap": "^9.0.2",
		"bootstrap": "^4.6.0",
		"core-js": "^3.9.0",
		"email-validator": "^2.0.4",
		"express": "^4.17.1",
		"file-saver": "^2.0.5",
		"moment": "^2.29.1",
		"moment-es6": "^1.0.0",
		"ng-click-outside": "^7.0.1",
		"npm": "^7.5.6",
		"rxjs": "^6.6.6",
		"tslib": "^2.1.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1102.2",
		"@angular/language-service": "^11.2.3",
		"@angularclass/hmr": "^3.0.0",
		"@types/jasmine": "^3.6.4",
		"@types/node": "^14.14.31",
		"gulp": "^4.0.2",
		"gulp-download-stream": "0.0.19",
		"jasmine-core": "^3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"karma": "^6.1.1",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "^3.0.3",
		"karma-firefox-launcher": "^2.1.0",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"karma-phantomjs-launcher": "^1.0.4",
		"ngx-i18nsupport": "^0.17.1",
		"prettier": "^2.2.1",
		"protractor": "^7.0.0",
		"puppeteer": "^7.1.0",
		"terser": "^5.6.0",
		"ts-node": "~9.1.1",
		"tslint": "^6.1.3",
		"typescript": "4.1"
	},
	"description": "`bl-web` is a customer centered application for Boklisten.no. It is basicly a online store with some caveats. This README only describes development and technical detail. For a deep-dive into the application [please read our documentation](https://github.com/boklisten/bl-doc/blob/master/bl-web/summary.md).",
	"main": "gulpfile.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/boklisten/bl-web.git"
	},
	"author": "",
	"bugs": {
		"url": "https://github.com/boklisten/bl-web/issues"
	},
	"homepage": "https://github.com/boklisten/bl-web#readme"
}
